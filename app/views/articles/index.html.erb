<%= render "shared/navbar" %>

<div class="container">
  <div class="row">
    <div class="col-sm-8">
      <% @articles.each do |article| %>
        <div class="article">
          <div class="article-title">
            <%=link_to article.title, article_path(article) %>
          </div>
          <div class="article-content">
            <%= article.content%>
          </div>
      <% end %>
        </div>
    </div>
  </div>
</div>
